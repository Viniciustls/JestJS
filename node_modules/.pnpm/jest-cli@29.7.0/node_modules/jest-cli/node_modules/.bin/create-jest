#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/create-jest@29.7.0/node_modules/create-jest/bin/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/create-jest@29.7.0/node_modules/create-jest/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/create-jest@29.7.0/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules:/home/vinicius/Documents/Cursos/node_modules:/home/vinicius/Documents/node_modules:/home/vinicius/node_modules:/home/node_modules:/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/jest-cli@29.7.0/node_modules/jest-cli/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/jest-cli@29.7.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/create-jest@29.7.0/node_modules/create-jest/bin/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/create-jest@29.7.0/node_modules/create-jest/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/create-jest@29.7.0/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules:/home/vinicius/Documents/Cursos/node_modules:/home/vinicius/Documents/node_modules:/home/vinicius/node_modules:/home/node_modules:/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/jest-cli@29.7.0/node_modules/jest-cli/node_modules:/home/vinicius/Documents/Cursos/JestJS/node_modules/.pnpm/jest-cli@29.7.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../create-jest@29.7.0/node_modules/create-jest/bin/create-jest.js" "$@"
else
  exec node  "$basedir/../../../../../create-jest@29.7.0/node_modules/create-jest/bin/create-jest.js" "$@"
fi
